{
  "struct": {
    "prefix": "str",
    "description": "typedef struct name {};",
    "body": [
			"typedef struct $1 {",
			"  $0;",
			"} $1;"
		]
  },
  "union": {
    "prefix": "uni",
    "description": "typedef union name {};",
    "body": [
			"typedef union $1 {",
			"  $0;",
			"} $1;"
		]
  },
  "enum": {
    "prefix": "enum",
    "description": "typedef enum name {};",
    "body": [
			"typedef enum $1 {",
			"  $0 = 0,",
			"} $1;"
		]
  },
  "function": {
    "prefix": "fn",
    "description": "return_type name(args..) {}",
    "body": [
			"${1:return_type} ${2:function_name}(${3:args}) {",
			"  $0",
			"}"
		]
  },
  "main": {
    "prefix": "main",
    "description": "int main(...) {}",
    "body": [
			"#include <stdio.h>",
			"",
      "int main(int argc, const char *argv[]) {",
      "  $0",
      "",
      "  return 0;",
			"}"
		]
  },
  "ternary": {
    "prefix": "tern",
    "description": "ternary operator",
    "body": "${1:expression} ? ${2:first_condition} : ${3:second_condition};",
	}
}
