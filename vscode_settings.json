{
    "workbench.colorTheme": "GitHub Dark Default",
    "editor.fontSize": 13,
    "window.zoomLevel": -1,
    "explorer.confirmDelete": false,
    "cmake.configureOnOpen": true,
    "workbench.iconTheme": "material-icon-theme",
    "telemetry.telemetryLevel": "off",
    "editor.minimap.showSlider": "always",
    "editor.minimap.renderCharacters": false,
    "editor.smoothScrolling": true,
    "editor.fontFamily": "'Source Code Pro', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "window.dialogStyle": "custom",
    "window.titleBarStyle": "custom",
    "workbench.sideBar.location": "right",
    "clangd.checkUpdates": true,
    "material-icon-theme.showUpdateMessage": true,
    "vim.enableNeovim": true,
    "vim.highlightedyank.enable": true,
    "vim.hlsearch": true,
    "vim.leader": " ",
    "vim.neovimPath": "/usr/bin/nvim",
    "vim.neovimConfigPath": "~/.config/nvim/init.vim",
    "vim.neovimUseConfigFile": true,
    "vim.useSystemClipboard": true,
    "vim.vimrc.enable": true,
    "keyboard.dispatch": "keyCode",
    "git.confirmSync": false,
    "clangd.arguments": ["--header-insertion=never"],
    "editor.semanticHighlighting.enabled": true,
    "editor.smoothScrolling": true,
    "mesonbuild.configureOnOpen": true,
    "editor.suggest.showStatusBar": true,
    "problems.showCurrentInStatus": true,
    "vim.showmodename": false,
    "debug.openExplorerOnEnd": true,
    "debug.showInStatusBar": "always",
    "git-graph.showStatusBarItem": false,
    "workbench.activityBar.visible": false,
    "update.mode": "none",
    "workbench.settings.enableNaturalLanguageSearch": false,
    "workbench.enableExperiments": false,
    "json.schemaDownload.enable": false,
    "extensions.autoCheckUpdates": false,
    "extensions.autoUpdate": false,
    "npm.fetchOnlinePackageInfo": false,
    "typescript.disableAutomaticTypeAcquisition": true,
    "editor.rulers": [80],
    "workbench.colorCustomizations": { "editorRuler.foreground": "#0000000e" },
    "editor.minimap.maxColumn": 80
}
